<h1> Articles </h1>
<h2> Your Articles for Today: </h2>
  <ul>
    <% @articles.each do |article| %>
    <li><%= link_to article.title, articles_path(article)%></li>
    <%= link_to "Delete", articles_path(article),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
    <% end %>
    </ul>
    <li> <%= link_to "Create New Article", new_path%></li>
